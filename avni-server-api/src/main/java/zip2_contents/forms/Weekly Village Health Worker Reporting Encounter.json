  "decisionRule" : "",
  "visitScheduleRule" : "\"use strict\";\n({ params, imports }) => {\n  const encounter = params.entity;\n  const moment = imports.moment;\n  const scheduleBuilder = new imports.rulesConfig.VisitScheduleBuilder({encounter});\n  \n  if(moment(encounter.earliestVisitDateTime).format('dddd') === 'Saturday' ){\n    const earliestDate = moment(encounter.earliestVisitDateTime).day(6).add(7,'days').toDate();\n    const maxDate = moment(earliestDate).add(1, 'days').toDate();\n    scheduleBuilder.add({name: \"Weekly Village Health Worker Reporting\", encounterType: \"Weekly Village Health Worker Reporting\", earliestDate, maxDate});  \n} else {\n    \n    const earliestDate = moment(encounter.earliestVisitDateTime).day(6).toDate();\n    const maxDate = moment(earliestDate).add(1, 'days').toDate();\n    scheduleBuilder.add({name: \"Weekly Village Health Worker Reporting\", encounterType: \"Weekly Village Health Worker Reporting\", earliestDate, maxDate});  \n}\n  \n  return scheduleBuilder.getAll();\n};",
